		Here_now_C2R_accN or (not Here_now_C2R_mvN0) or Here_will_C2R_mvN1
		(not Here_now_C2R_accN) or (not Here_now_C2R_mvN0) or Here_will_C2R_mvN1
        //model.addConstr(    ( Here_now_C2R_mvN3                          ) REQUIRES      ( N_will_C2R_ready  OR  N_will_C2R_mvN0 )          );
		(not Here_now_C2R_mvN3) or N_will_C2R_ready  or  N_will_C2R_mvN0
        //model.addConstr(    ( Here_now_C2R_mvN3                          ) REQUIRES      ( Here_will_nobodyhome  OR  Here_will_R_mvN0 )     );
		(not Here_now_C2R_mvN3) or Here_will_nobodyhome  or  Here_will_R_mvN0
        //model.addConstr(    ( Here_will_C2R_accS                         ) REQUIRES      ( Here_now_C2R_ready )                             );
		(not Here_will_C2R_accS) or Here_now_C2R_ready
        //model.addConstr(    ( Here_will_C2R_mvS1                         ) EQUIVALENT_TO ( Here_now_C2R_accS  OR  Here_now_C2R_mvS0 )       ); // !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
			//( Here_will_C2R_mvS1                         ) IMPLIES ( Here_now_C2R_accS  OR  Here_now_C2R_mvS0 )
			(not Here_will_C2R_mvS1) or Here_now_C2R_accS  or  Here_now_C2R_mvS0
			//( Here_will_C2R_mvS1                         ) <= ( Here_now_C2R_accS  OR  Here_now_C2R_mvS0 )
