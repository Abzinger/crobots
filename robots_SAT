		//model.addConstr(    ( Here_will_C2R_accW                         ) REQUIRES      ( Here_now_C2R_ready )                             );
		(not  Here_will_C2R_accW) or Here_now_C2R_ready
        //model.addConstr(    ( Here_will_C2R_mvW1                         ) EQUIVALENT_TO ( Here_now_C2R_accW  OR  Here_now_C2R_mvW0 )       ); // !!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
		//( Here_will_C2R_mvW1                         ) IMPLIES ( Here_now_C2R_accW  OR  Here_now_C2R_mvW0 )
		(not Here_will_C2R_mvW1) or Here_now_C2R_accW or Here_now_C2R_mvW0
		//( Here_will_C2R_mvW1                         ) <= ( Here_now_C2R_accW  OR  Here_now_C2R_mvW0 )
		(not Here_now_C2R_accW) or Here_now_C2R_mvW0 or Here_will_C2R_mvW1
		Here_now_C2R_accW or (not Here_now_C2R_mvW0) or Here_will_C2R_mvW1
		(not Here_now_C2R_accW) or (not Here_now_C2R_mvW0) or Here_will_C2R_mvW1
        //model.addConstr(    ( Here_will_C2R_mvW0                         ) REQUIRES      ( E_now_C2R_mvW1    )                              );
		(not Here_will_C2R_mvW0) or E_now_C2R_mvW1
        //model.addConstr(    ( Here_now_C2R_accW   OR   Here_now_C2R_mvW0 ) REQUIRES      ( Here_will_C2R_mvW1 )                             );
		(not Here_now_C2R_accW) or Here_now_C2R_mvW0 or Here_will_C2R_mvW1
		Here_now_C2R_accW or (not Here_now_C2R_mvW0) or Here_will_C2R_mvW1
		(not Here_now_C2R_accW) or (not Here_now_C2R_mvW0) or Here_will_C2R_mvW1
        //model.addConstr(    ( Here_now_C2R_mvW1                          ) REQUIRES      ( W_will_C2R_ready  OR  W_will_C2R_mvW0 )          );
