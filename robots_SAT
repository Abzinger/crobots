        //model.addConstr(    ( Here_now_C2R_mvW1                          ) REQUIRES      ( Here_will_nobodyhome  OR  Here_will_R_mvW0 )     );
		(not Here_now_C2R_mvW1) or Here_will_nobodyhome  or  Here_will_R_mvW0
	}
// L I F T I N G + D R O P P I N G
    {	// Lifting process
		// Abbreviation:
        //const GRBLinExpr Here_will_CR_ready = Here_will_C0R_ready + Here_will_C1R_ready + Here_will_C2R_ready;
			//Here_will_CR_ready IMPLIES Here_will_C0R_ready + Here_will_C1R_ready + Here_will_C2R_ready
			(not Here_will_CR_ready) or Here_will_C0R_ready or Here_will_C1R_ready or Here_will_C2R_ready
			//Here_will_CR_ready <= Here_will_C0R_ready + Here_will_C1R_ready + Here_will_C2R_ready
			(not Here_will_C0R_ready) or Here_will_C1R_ready or Here_will_C2R_ready or Here_will_CR_ready
			Here_will_C0R_ready or (not Here_will_C1R_ready) or Here_will_C2R_ready or Here_will_CR_ready
			Here_will_C0R_ready or Here_will_C1R_ready or (not Here_will_C2R_ready) or Here_will_CR_ready
			(not Here_will_C0R_ready) or (not Here_will_C1R_ready) or Here_will_C2R_ready or Here_will_CR_ready
			(not Here_will_C0R_ready)or Here_will_C1R_ready or (not Here_will_C2R_ready) or Here_will_CR_ready
			Here_will_C0R_ready or (not Here_will_C1R_ready) or (not Here_will_C2R_ready) or Here_will_CR_ready
			(not Here_will_C0R_ready) or (not Here_will_C1R_ready) or (not Here_will_C2R_ready) or Here_will_CR_ready
		
		//model.addConstr( Here_now_R_ready     FOLLOWS_FROM   Here_will_R_lift        );
		(not Here_will_R_lift) or Here_now_R_ready

        //model.addConstr( Here_now_R_lift      EQUIVALENT_TO  Here_will_R_lifting1    );
		(not Here_now_R_lift) or  Here_will_R_lifting1
		Here_now_R_lift or  (not Here_will_R_lifting1)
        //model.addConstr( Here_now_R_lifting1  EQUIVALENT_TO  Here_will_R_lifting2    );
		(not Here_now_R_lifting1) or  Here_will_R_lifting2
		Here_now_R_lifting1 or  (not Here_will_R_lifting2)
        //model.addConstr( Here_now_R_lifting2  EQUIVALENT_TO  Here_will_R_lifting3    );
		(not Here_now_R_lifting2) or  Here_will_R_lifting3
		Here_now_R_lifting2 or  (not Here_will_R_lifting3)
        //model.addConstr( Here_now_R_lifting3  EQUIVALENT_TO  Here_will_R_lifting4    );
		(not Here_now_R_lifting3) or  Here_will_R_lifting4
		Here_now_R_lifting3 or  (not Here_will_R_lifting4)

        //model.addConstr( Here_will_CR_ready   IMPLIES   Here_now_empty OR Here_now_R_lifting4 );
		(not Here_will_CR_ready) or Here_now_empty or Here_now_R_lifting4

        //model.addConstr( Here_now_R_lifting4   IMPLIES   Here_will_C0R_ready OR Here_now_C1         OR Here_now_C2     );  // maybe make these lazy?!?
		(not Here_now_R_lifting4) or Here_will_C0R_ready or Here_now_C1 or Here_now_C2
        //model.addConstr( Here_now_R_lifting4   IMPLIES   Here_now_C0         OR Here_now_C1         OR Here_will_C2R_ready );
		(not Here_now_R_lifting4) or Here_now_C0 or OR Here_now_C1 or Here_will_C2R_ready
        //model.addConstr( Here_now_R_lifting4   IMPLIES   Here_will_C0R_ready OR Here_now_C1         OR Here_will_C2R_ready );
		(not Here_now_R_lifting4) or Here_will_C0R_ready or Here_now_C1 or Here_will_C2R_ready
        //model.addConstr( Here_now_R_lifting4   IMPLIES   Here_will_C0R_ready OR Here_will_C1R_ready OR Here_now_C2     );
		(not Here_now_R_lifting4) or Here_will_C0R_ready or Here_will_C1R_ready or Here_now_C2 
        //model.addConstr( Here_now_R_lifting4   IMPLIES   Here_now_C0         OR Here_will_C1R_ready OR Here_now_C2     );
		(not Here_now_R_lifting4) or Here_now_C0 or Here_will_C1R_ready or Here_now_C2
        //model.addConstr( Here_now_R_lifting4   IMPLIES   Here_now_C0         OR Here_will_C1R_ready OR Here_will_C2R_ready );
		(not Here_now_R_lifting4)  or Here_now_C0 or Here_will_C1R_ready or Here_will_C2R_ready
        //model.addConstr( Here_now_R_lifting4   IMPLIES   Here_will_C0R_ready OR Here_will_C1R_ready OR Here_will_C2R_ready );
		(not Here_now_R_lifting4) or Here_will_C0R_ready or Here_will_C1R_ready or Here_will_C2R_ready
}
	{	// Dropping process
		//model.addConstr( Here_now_R_drop IMPLIES Here_will_R_ready );
		(not Here_now_R_drop) or Here_will_R_ready
        //model.addConstr( Here_now_C0R_ready OR Here_now_C1R_ready OR Here_now_C2R_ready   IMPLIES   Here_will_empty OR Here_will_R_drop );
		(not Here_will_C0R_ready) or Here_will_C1R_ready or Here_will_C2R_ready or Here_will_empty or Here_will_R_drop
			Here_will_C0R_ready or (not Here_will_C1R_ready) or Here_will_C2R_ready or Here_will_empty or Here_will_R_drop
			Here_will_C0R_ready or Here_will_C1R_ready or (not Here_will_C2R_ready) or Here_will_empty or Here_will_R_drop
			(not Here_will_C0R_ready) or (not Here_will_C1R_ready) or Here_will_C2R_ready or Here_will_empty or Here_will_R_drop
			(not Here_will_C0R_ready)or Here_will_C1R_ready or (not Here_will_C2R_ready) or Here_will_empty or Here_will_R_drop
			Here_will_C0R_ready or (not Here_will_C1R_ready) or (not Here_will_C2R_ready) or Here_will_empty or Here_will_R_drop
			(not Here_will_C0R_ready) or (not Here_will_C1R_ready) or (not Here_will_C2R_ready) or Here_will_empty or Here_will_R_drop
        //model.addConstr( Here_will_R_drop   IMPLIES   Here_will_C0        OR Here_will_C1        OR Here_will_C2   );  // maybe make these lazy?!?
		(not Here_now_R_drop) or Here_will_C0 or Here_will_C1 or Here_will_C2
        //model.addConstr( Here_will_R_drop   IMPLIES   Here_now_C0R_ready  OR Here_will_C1        OR Here_will_C2   );
		(not Here_now_R_drop) or Here_now_C0R_ready or Here_will_C1 or Here_will_C2
        //model.addConstr( Here_will_R_drop   IMPLIES   Here_will_C0        OR Here_will_C1        OR Here_now_C2R_ready );
		(not Here_now_R_drop) or Here_will_C0 or Here_will_C1 or Here_now_C2R_ready
        //model.addConstr( Here_will_R_drop   IMPLIES   Here_now_C0R_ready  OR Here_will_C1        OR Here_now_C2R_ready );
		(not Here_now_R_drop) or Here_now_C0R_ready or Here_will_C1 or Here_now_C2R_ready
        //model.addConstr( Here_will_R_drop   IMPLIES   Here_now_C0R_ready  OR Here_now_C1R_ready  OR Here_will_C2   );
