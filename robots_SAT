        //model.addConstr( Here_will_R_drop   IMPLIES   Here_will_C0        OR Here_now_C1R_ready  OR Here_will_C2   );
		(not Here_now_R_drop) or Here_will_C0 or Here_now_C1R_ready or Here_will_C2
        //model.addConstr( Here_will_R_drop   IMPLIES   Here_will_C0        OR Here_now_C1R_ready  OR Here_now_C2R_ready );
		(not Here_now_R_drop) or Here_will_C0 or Here_now_C1R_ready or Here_now_C2R_ready
        //model.addConstr( Here_will_R_drop   IMPLIES   Here_now_C0R_ready  OR Here_now_C1R_ready  OR Here_now_C2R_ready );
		(not Here_now_R_drop) or Here_now_C0R_ready or Here_now_C1R_ready or Here_now_C2R_ready
    }
	{	// Lift/drop and  PARKED cars
		//model.addConstr( Here_now_R_lifting4 + Here_now_empty ==   Here_will_R_drop  + Here_will_empty );
		//( Here_now_R_lifting4 + Here_now_empty =>   Here_will_R_drop  + Here_will_empty )
		(not Here_now_R_lifting4) or Here_now_empty or Here_will_R_drop  or Here_will_empty
		 Here_now_R_lifting4 or (not Here_now_empty) or Here_will_R_drop  or Here_will_empty
		(not Here_now_R_lifting4) or (not Here_now_empty) or Here_will_R_drop  or Here_will_empty
		//( Here_now_R_lifting4 + Here_now_empty <=   Here_will_R_drop  + Here_will_empty )
		(not Here_will_R_drop)  or Here_will_empty or Here_now_R_lifting4 or Here_now_empty
		Here_will_R_drop  or (not Here_will_empty) or Here_now_R_lifting4 or Here_now_empty
		(not Here_will_R_drop)  or (not Here_will_empty) or Here_now_R_lifting4 or Here_now_empty
        //model.addConstr( ( Here_now_R_drop      OR Here_will_R_drop    ) IMPLIES   NOT Here_will_empty );
		(not Here_now_R_drop) or Here_will_R_drop or (not Here_will_empty)
		Here_now_R_drop or (not Here_will_R_drop) or (not Here_will_empty)
		(not Here_now_R_drop) or (not Here_will_R_drop) or (not Here_will_empty)
        //model.addConstr( ( Here_will_R_lifting4 OR Here_now_R_lifting4 ) IMPLIES   NOT Here_now_empty  );
		(not Here_will_R_lifting4) or Here_now_R_lifting4 or (not Here_now_empty)
		Here_will_R_lifting4 or (not Here_now_R_lifting4) or (not Here_now_empty)
		(not Here_will_R_lifting4) or (not Here_now_R_lifting4) or (not Here_now_empty)
        //model.addConstr( Here_will_R_drop    IMPLIES   Here_now_empty );
		(not Here_will_R_drop) or Here_now_empty
        //model.addConstr( Here_now_R_lifting4 IMPLIES   Here_will_empty );
		(not  Here_now_R_lifting4) or Here_will_empty
        //model.addConstr( Here_will_C0  OR  Here_will_C1    IMPLIES    Here_now_C0  OR  Here_now_C1  OR  Here_will_R_drop  );
		(not Here_will_C0)  or  Here_will_C1 or Here_now_C0 or Here_now_C1 or Here_will_R_drop
		Here_will_C0  or  (not Here_will_C1) or Here_now_C0 or Here_now_C1 or Here_will_R_drop
		(not Here_will_C0)  or  (not Here_will_C1) or Here_now_C0 or Here_now_C1 or Here_will_R_drop
        //model.addConstr( Here_will_C0  OR  Here_will_C2    IMPLIES    Here_now_C0  OR  Here_now_C2  OR  Here_will_R_drop  );
		(not Here_will_C0)  or  Here_will_C2 or Here_now_C0 or Here_now_C2 or Here_will_R_drop
		Here_will_C0  or  (not Here_will_C2) or Here_now_C0 or Here_now_C2 or Here_will_R_drop
		(not Here_will_C0)  or  (not Here_will_C2) or Here_now_C0 or Here_now_C2 or Here_will_R_drop
        //model.addConstr( Here_will_C1  OR  Here_will_C2    IMPLIES    Here_now_C1  OR  Here_now_C2  OR  Here_will_R_drop  );
		(not Here_will_C1)  or  Here_will_C2 or Here_now_C1 or Here_now_C2 or Here_will_R_drop
		Here_will_C1  or  (not Here_will_C2) or Here_now_C1 or Here_now_C2 or Here_will_R_drop
		(not Here_will_C1)  or  (not Here_will_C2) or Here_now_C1 or Here_now_C2 or Here_will_R_drop
        //model.addConstr( Here_now_C0   OR  Here_now_C1     IMPLIES    Here_will_C0  OR  Here_will_C1  OR  Here_now_R_lifting4 );
		(not Here_now_C0)  or  Here_now_C1 or Here_will_C0 or Here_will_C1 or Here_will_R_lifting4
		Here_now_C0  or  (not Here_now_C1) or Here_will_C0 or Here_will_C1 or Here_will_R_lifting4
		(not Here_now_C0)  or  (not Here_now_C1) or Here_will_C0 or Here_will_C1 or Here_will_R_lifting4
        //model.addConstr( Here_now_C0   OR  Here_now_C2     IMPLIES    Here_will_C0  OR  Here_will_C2  OR  Here_now_R_lifting4 );
		(not Here_now_C0)  or  Here_now_C2 or Here_will_C0 or Here_will_C2 or Here_will_R_lifting4
		Here_now_C0  or  (not Here_now_C2) or Here_will_C0 or Here_will_C2 or Here_will_R_lifting4
		(not Here_now_C0)  or  (not Here_now_C2) or Here_will_C0 or Here_will_C2 or Here_will_R_lifting4
        //model.addConstr( Here_now_C1   OR  Here_now_C2     IMPLIES    Here_will_C1  OR  Here_will_C2  OR  Here_now_R_lifting4 );
		(not Here_now_C1)  or  Here_now_C2 or Here_will_C1 or Here_will_C2 or Here_will_R_lifting4
		Here_now_C1  or  (not Here_now_C2) or Here_will_C1 or Here_will_C2 or Here_will_R_lifting4
		(not Here_now_C1)  or  (not Here_now_C2) or Here_will_C1 or Here_will_C2 or Here_will_R_lifting4

        //model.addConstr( NOT Here_now_empty  IMPLIES   NOT Here_will_empty  OR  Here_now_R_lifting4 );
		Here_now_empty or (not Here_will_empty) or (Here_now_R_lifting4)

    }


